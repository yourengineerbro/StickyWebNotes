{
    "manifest_version": 3,
    "name": "Sticky Web Notes",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  